{  
  "version": 1,  
  "disable_existing_loggers": false,  
  "formatters": {  
    "simple": {  
      "format": "%(filename)s: %(asctime)s : %(name)s : %(levelname)s : %(message)s"  
  }  
  },  
  "handlers": {  
    "file_handler": {  
      "class": "logging.FileHandler",  
  "level": "DEBUG",  
  "formatter": "simple",  
  "filename": "logs.log",  
  "encoding": "utf8"  
  }  
  },  
  "root": {  
    "level": "DEBUG",  
  "handlers": ["file_handler"]  
  }  
}
